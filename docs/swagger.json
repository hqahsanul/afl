  "definitions": {
    
    "user_signup": {
      "type": "object",
      "required": [
        "name",
        "email",
        "mobile",
        "password",
        "confirm_password"
        
      ],
      "properties": {
        "name": {
          "type": "string",
          "example": "John Doe"
        },
        "email": {
          "type": "string",
          "example": "abc@example.com"
        },
        "mobile": {
          "type": "string",
          "example": "9089909090"
        },
        "password": {
          "type": "string",
          "example": "abc@123"
        },
        "confirm_password": {
          "type": "string",
          "example": "abce@123"
        }
       
      }
    },

    "user_profile": {
      "type": "object",
      "required": [
        "_id",
        "shippingAddress1",
        "shippingAddress2",
        "city",
        "zipCode",
        "state",
        "secondaryPhone",
        "licenseNumber"
      ],
      "properties": {
        "_id": {
          "type": "string",
          "example": "5ed660d63bbd282d947af1c6"
        },
        "shippingAddress1": {
          "type": "string",
          "example": "Avenue street"
        },
        "shippingAddress2": {
          "type": "string",
          "example": "3 mall"
        },
        "state": {
          "type": "string",
          "example": "5e15b6f7dbc1a60870ed864e"
        },
        "city": {
          "type": "string",
          "example": "Florida"
        },
        "zipCode": {
          "type": "string",
          "example": "123456"
        },
        "secondaryPhone": {
          "type": "string",
          "example": "(123) 345 7890"
        },
        "licenseNumber": {
          "type": "String",
          "example": "123Fgj"
        }
      }
    },

    "verify_otp": {
      "type": "object",
      "required": [
        "otp",
        "resetToken",
        "email"
      ],
      "properties": {
        "resetToken": {
          "type": "string",
          "description": "Auth token",
          "example": "fdf056adc9846820a9d520"
        },
        "otp": {
          "type": "string",
          "description": "Recieved on submitted mobile number",
          "example": "1234"
        },
        "email": {
          "type": "string",
          "description": "User email",
          "example": "abc@example.com"
        }
      }
    },

    "add_wallet": {
      "type": "object",
      "required": [
        "balance"
        
      ],
      "properties": {
        "balance": {
          "type": "number",
          "example": "100"
        }
       
      }
    },

    "add_game": {
      "type": "object",
      "required": [
        "gameName",
